import numpy
from numpy cimport abs

cdef extern from 'foo.h':
    int foo_int
    struct foo_struct:
        pass

ctypedef int word

cdef struct Foo:
    pass


cpdef fib(int n):    # Cython version
    """Print a Fibonacci series up to n."""
    cdef int a = 0
    cdef int b

    b = 1

    while a < n:
        print(a)
        a, b = b, a+b

# Python function
def fib(n):    # Python version
    """Print a Fibonacci series up to n."""
    a, b = 0, 1
    while a < n:
        print a,
        a, b = b, a+b
