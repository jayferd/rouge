@interface Person : NSObject {
  @public
  NSString *name;
  @private
  int age;
}

@property(copy) NSString *name;
@property(readonly) int age;

-(id)initWithAge:(int)age;
@end

@implementation Person
-(void)doStuff {
    [self executeBlock:^void(int x, NSString *y) {
                          [self confirmPayment];
                       }];
}
@end

